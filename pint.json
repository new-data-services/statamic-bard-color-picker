{
    "preset": "laravel",
    "rules": {
        "binary_operator_spaces": {
            "default": "align",
            "operators": {
                "??": "at_least_single_space",
                "||": "at_least_single_space"
            }
        },
        "concat_space": {
            "spacing": "one"
        }
    }
}
